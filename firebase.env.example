## Runtime Firebase configuration for Flutter (used by FirebaseConfig)
#
# Usage:
#   flutter run --dart-define-from-file=firebase.env
#   flutter build apk --dart-define-from-file=firebase.env
#   flutter build web --dart-define-from-file=firebase.env
#
# Required
FIREBASE_API_KEY=
FIREBASE_APP_ID=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_PROJECT_ID=

# Optional (recommended when available for your platform)
FIREBASE_AUTH_DOMAIN=
FIREBASE_DATABASE_URL=
FIREBASE_STORAGE_BUCKET=
FIREBASE_IOS_BUNDLE_ID=

# Notes:
# - These values should be copied from Firebase Console > Project settings > Your apps
# - Using this runtime config lets you run with correct keys without editing code
# - If not provided, the app falls back to lib/firebase_options.dart

